{
  "name": "@viennt/vite-plugin-collector",
  "version": "0.1.0",
  "packageManager": "pnpm@7.12.0",
  "description": "Module collector for Vite",
  "author": "viennt <thevienit@gmail.com>",
  "license": "MIT",
  "bugs": "https://github.com/viennt/vite-plugin-collector/issues",
  "homepage": "https://github.com/viennt/vite-plugin-collector#readme",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/viennt/vite-plugin-collector.git"
  },
  "keywords": [
    "vite",
    "plugin"
  ],
  "scripts": {
    "commit": "cz",
    "dev": "esno scripts/dev.ts",
    "build": "esno scripts/build.ts",
    "publish:ci": "esno scripts/publish.ts",
    "prepublishOnly": "npm run build",
    "release": "npx bumpp --push --tag --commit && npm publish",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "vite": "^4.0.0",
    "workbox-build": "^6.5.4",
    "workbox-window": "^6.5.4"
  },
  "dependencies": {
    "@types/micromatch": "^4.0.2",
    "debug": "^4.3.4",
    "fast-glob": "^3.2.11",
    "micromatch": "^4.0.5",
    "pretty-bytes": "^6.0.0",
    "stringify-object": "^4.0.1",
    "workbox-build": "^6.5.4",
    "workbox-window": "^6.5.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.26.3",
    "@antfu/ni": "^0.18.0",
    "@antfu/utils": "^0.5.2",
    "@types/debug": "^4.1.7",
    "@types/node": "^18.7.14",
    "@types/prompts": "^2.4.0",
    "@types/stringify-object": "^4.0.1",
    "@types/workbox-build": "^5.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "commitizen": "^4.2.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.23.0",
    "esno": "0.16.3",
    "prompts": "^2.4.2",
    "rollup": "^2.79.0",
    "semantic-release": "^19.0.5",
    "tsup": "^6.2.3",
    "typescript": "^4.8.2",
    "vite": "^4.0.0",
    "vue": "^3.2.38"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
